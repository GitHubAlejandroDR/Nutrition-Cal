pipeline {
    agent none
    stages {
        stage('Build') {
            echo "Build Stage"
            
            steps {
                sh 'python sources/calc.py'
            }
        }
        stage('Test') {
           
            steps {
                sh 'python sources/test.calorias.py'
            }
           
            
        }
        stage('Deliver') {
            
            steps {           
                    sh 'python sources/Calory_cal.py'
            }
            post {
                echo "Deliver stage"
            }
        }
    }
}
